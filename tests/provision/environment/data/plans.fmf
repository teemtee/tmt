provision:
  - how: container
    name: default-0
    environment:
      SOME_ENVVAR_FROM_GUEST_ENVIRONMENT: foo
  - how: container
    name: default-1
    environment:
      SOME_ENVVAR_FROM_GUEST_ENVIRONMENT: bar

execute:
    how: tmt
    script: |
      . "$TMT_TOPOLOGY_BASH"
      echo ${TMT_GUEST[name]}:$SOME_ENVVAR_FROM_GUEST_ENVIRONMENT

/test-override:
    environment:
      SOME_ENVVAR_FROM_GUEST_ENVIRONMENT: baz

/guest-only:
    # Uses shared provision
