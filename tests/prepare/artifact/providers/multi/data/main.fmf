/plan:
    discover:
        how: fmf
    provision:
        how: container
    prepare:
      - how: artifact
        provide:
          # DUMMY_KOJI_BUILD_ID placeholder - replace with actual koji build ID
          # Example: koji.build:2761810
          - koji.build:DUMMY_KOJI_BUILD_ID
          - repository-file:https://download.docker.com/linux/fedora/docker-ce.repo
    execute:
        how: tmt

/test:
    test: ./test.sh
    duration: 5m
    summary: Verify packages from multiple providers
    require:
      - make
      - docker-ce-cli
